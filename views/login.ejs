<!DOCTYPE html>
<html lang="ko">
  <head>
    <%- include ('./common/head') %>
    <title>N-blog</title>
  </head>
  <body>
    <%- include ('./common/header') %>

    <div class="wrap">
      <section class="sign-up">
        <img src="images/team_work_flatline.svg" alt="" class="sign-up__img" />
        <div>
          <h1 class="sign-up__title">로그인</h1>
          <form action="" method="post" class="sign-up__form">
            <label for="email">이메일</label>
            <input type="email" id="email" name="email" class="input" />

            <label for="password">비밀번호</label>
            <input type="password" id="password" name="pw" class="input" />

            <button type="submit" class="btn btn-submit">확인</button>
          </form>
        </div>
      </section>
    </div>
  </body>
</html>
